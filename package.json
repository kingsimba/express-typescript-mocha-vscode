{
    "name": "express-typescript",
    "version": "1.1.0",
    "description": "Develop and ExpressJS webserver using TypeScript",
    "main": "dist/index.js",
    "scripts": {
        "build": "npx tsc",
        "docker:build": "docker build . -t simba/my_server --target my_server",
        "watch": "ts-node-dev --respawn src/index.ts",
        "test": "mocha src/**/*.ts --require='ts-node/register/transpile-only'",
        "kill": "TaskKill /IM node.exe /F",
        "lint": "eslint -c .eslintrc.js --ext .ts src"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cookie-parser": "^1.4.5",
        "express": "^4.17.3",
        "guid-typescript": "^1.0.9",
        "request": "^2.88.2"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/chai": "^4.2.11",
        "@types/express": "^4.17.3",
        "@types/mocha": "^7.0.2",
        "@types/node": "^12.12.31",
        "@types/request": "^2.48.4",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "chai": "^4.2.0",
        "chai-http": "^4.2.0",
        "eslint": "^8.5.0",
        "eslint-plugin-jsdoc": "^37.4.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "mocha": "^9.1.3",
        "ts-node": "^8.8.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.3"
    }
}